{{~it.results :syllables }}
  <div>
    <div>
      {{=syllables.syllables}}
    </div>
    <div>
      {{~syllables.rhymes :rhyme }}
        <div>
          <div>
            <!-- {{=rhyme.rhyme}} -->
          </div>
          <div>
            {{~rhyme.words :word }}
              <span class="s_{{=word.score}}">{{=word.word}}</span>,
            {{~}}
          </div>
        </div>
      {{~}}
    </div>
  </div>
{{~}}
