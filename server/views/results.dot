{{~it.results :syllables }}
  <div class="syllables">
    <div class="syllable">
      {{=syllables.syllables}}
    </div>
    <div class="rhymes">
      {{~syllables.rhymes :rhyme }}
        <div>
          <div>
            <!-- {{=rhyme.rhyme}} -->
          </div>
          <div class="">
            {{~rhyme.words :word:index }}
              <span class="s_{{=word.score}}">{{=word.word}}</span>{{? index == rhyme.words.length }},{{?}}
            {{~}}
          </div>
        </div>
      {{~}}
    </div>
  </div>
{{~}}
